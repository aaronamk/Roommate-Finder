<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".PersonalityTest">

    <WebView
        android:id="@+id/personalityTestLink"
        android:layout_width="634dp"
        android:layout_height="657dp"
        app:layout_constraintBottom_toTopOf="@id/editPersonalityType"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <EditText
        android:id="@+id/editPersonalityType"
        android:layout_width="411dp"
        android:layout_height="74dp"
        android:layout_marginTop="108dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:hint="@string/insert_personality_type"
        android:inputType="text"
        android:textColor="#918686"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/personalityTestLink" />

    <Button
        android:id="@+id/nextButton"
        android:layout_width="285dp"
        android:layout_height="58dp"
        android:layout_marginTop="148dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/next"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editPersonalityType" />


</androidx.constraintlayout.widget.ConstraintLayout>
